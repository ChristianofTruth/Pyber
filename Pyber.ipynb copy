    "#Percentage of Total Fare by City Types\n",
    "\n",
    "city_type_df = merge_table.groupby([\"type\"])\n",
    "\n",
    "labels = city_type_df[\"type\"].unique()\n",
    "\n",
    "total_fare = city_type_df[\"fare\"].sum()\n",
    "\n",
    "colors = [\"gold\", \"lightskyblue\", \"lightcoral\"]\n",
    "\n",
    "explode = (0, 0, 0.1)\n",
    "\n",
    "plt.pie(total_fare, explode=explode, labels=labels, colors=colors, \n",
    "        autopct=\"%1.1f%%\", shadow=False, startangle=153)\n",
    "plt.title(\"% of Total Fare by City Type\")"
   ]
