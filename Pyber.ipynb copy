    "rides_per_city = unique_types[\"ride_id\"]\n",
    "average_fare_per_city = city[\"fare\"].mean()\n",
    "s = city[\"driver_count\"].sum()\n",
    "\n",
    "colors = [\"gold\", \"lightskyblue\", \"lightcoral\"]\n",
    "\n",
    "x_limit = 42\n",
    "plt.xlim(0, x_limit)\n",
    "plt.yticks(np.arange(20, 45, 5))\n",
    "\n",
    "plt.title(\"Pyber Sharing Data (2016)\")\n",
    "plt.xlabel(\"Total Number of Rides (Per City)\")\n",
    "plt.ylabel(\"Average Fare ($)\")\n",
    "plt.text(42.5, 35, \" Note: Circle size correlates with driver count per city\", fontsize=10)\n",
    "\n",
    "\n",
    "plt.scatter(rides_per_city, average_fare_per_city, s=s, edgecolors=\"black\")\n",
    "plt.grid()\n",
    "plt.legend(handles=, loc='upper right')\n",
    "plt.show()\n",
